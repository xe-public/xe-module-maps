<query id="updateMapsContent" action="update">
	<tables>
		<table name="maps_contents" />
	</tables>
	<columns>
		<column name="member_srl" var="member_srl" />
		<column name="maps_content" var="maps_content" />
		<column name="title" var="title" />
		<column name="content" var="content" />
		<column name="ipaddress" var="ipaddress" />
		<column name="update" default="curdate()" />
	</columns>
	<conditions>
		<condition operation="equal" column="maps_srl" var="maps_srl" notnull="notnull" />
	</conditions>
</query>